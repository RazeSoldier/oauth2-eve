{
    "name": "dts-eve/oauth2-eve",
    "description": "EVE Online Oauth 2.0 Client Provider for The PHP League OAuth2-Client",
    "type": "library",
    "keywords" :[
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "eve",
        "eve online",
        "eveonline"
    ],
    "homepage": "https://github.com/dts-eve/oauth2-eve",
    "license": "MIT",
    "authors": [
        {
            "name": "Oizys",
            "email": "oizys@killmails.com",
            "homepage": "https://killmails.com"
        }
    ],
    "support": {
        "issues": "https://github.com/dts-eve/oauth2-eve/issues"
    },
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.7|^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DtsEve\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DtsEve\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
